<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.codeyang.auth.mapper.OrganizationMapper">


    <select id="getByNameEq" resultType="cn.codeyang.auth.entity.Organization">
        select *
        from t_organization
        where name = #{name}
    </select>
</mapper>